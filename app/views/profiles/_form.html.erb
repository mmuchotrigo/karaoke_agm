<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :apellido_paterno %>
    <%= f.input :apellido_materno %>
    <%= f.input :dni %>
    <%= f.input :telefono %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
